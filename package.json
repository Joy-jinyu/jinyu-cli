{
  "name": "@ovometajs/cli",
  "version": "0.0.1",
  "description": "hello cli project",
  "bin": {
    "ovo": "bin/ovo-cli.js"
  },
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir bin --extensions .ts",
    "rollup": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript --sourcemap",
    "dev": "ts-node --esm src/index.ts hello -n ovo",
    "hello": "node index.js hello -n ovo",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint:fix": "npx eslint --ext .ts src/ --fix"
  },
  "keywords": [
    "commander",
    "cli",
    "ovo",
    "ovometa"
  ],
  "author": "joy@ovometa.group",
  "license": "MIT",
  "dependencies": {
    "babel-jest": "^29.0.3",
    "chalk": "^4.1.2",
    "commander": "^9.4.0",
    "core-js": "3.25.3",
    "execa": "^6.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime-corejs3": "^7.20.1",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^23.0.3",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/commander": "^2.12.2",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.0.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "cjs/",
    "index.js",
    "index.d.ts",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
